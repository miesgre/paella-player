{
  "name": "@asicupv/paella-extra-plugins",
  "version": "2.5.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/paella-extra-plugins.es.d.ts",
      "import": "./dist/paella-extra-plugins.es.js",
      "require": "./dist/paella-extra-plugins.cjs.js"
    },
    "./paella-extra-plugins.css": {
      "import": "./dist/paella-extra-plugins.css",
      "require": "./dist/paella-extra-plugins.css"
    }
  },
  "description": "Extra plugins for Paella Player.",
  "scripts": {
    "dev": "tsc && vite build --watch",
    "build": "tsc && vite build --emptyOutDir"
  },
  "main": "./dist/paella-extra-plugins.es.js",
  "files": [
    "dist/paella-extra-plugins.css",
    "dist/paella-extra-plugins*.js",
    "dist/paella-extra-plugins*.js.map",
