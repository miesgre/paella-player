{
  "name": "@asicupv/paella-onboarding-plugin",
  "version": "2.2.11",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/paella-onboarding-plugin.es.d.ts",
      "import": "./dist/paella-onboarding-plugin.es.js",
      "require": "./dist/paella-onboarding-plugin.cjs.js"
    },
    "./paella-onboarding-plugin.css": {
      "import": "./dist/paella-onboarding-plugin.css",
      "require": "./dist/paella-onboarding-plugin.css"
    }
  },
  "description": "A plugin for Paella Player that provides onboarding functionality to guide users through the interface and features.",  
  "scripts": {
    "dev": "tsc && vite build --watch",
    "build": "tsc && vite build --emptyOutDir"
  },
  "main": "./dist/paella-onboarding-plugin.es.js",
  "files": [
    "dist/paella-onboarding-plugin.cjs.js",
    "dist/paella-onboarding-plugin.cjs.js.map",
    "dist/paella-onboarding-plugin.css",
    "dist/paella-onboarding-plugin.es.js",
    "dist/paella-onboarding-plugin.es.js.map",
    "dist/types/**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polimediaupv/paella-player.git"
  },
  "keywords": [
    "paella",
    "player",
    "onboarding",
    "help"
  ],
  "author": "Miguel Escriva Gregori <miesge@gmail.com>",
  "license": "SEE LICENSE IN license.txt",
  "bugs": {
    "url": "https://github.com/polimediaupv/paella-player/issues"
  },
  "homepage": "https://github.com/polimediaupv/paella-player#readme",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "typescript": "~5.8.3",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@asicupv/paella-core": "^2.2.11",
    "shepherd.js": "^14.5.0"
  }
}
